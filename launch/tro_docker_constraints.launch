<launch>
  <arg name="no_of_agents"  default="3"/>
  <node pkg="cbf_constraints" name="master_task_node" type="task_assigner.py" output="screen">
    <param name="no_of_agents" value="$(arg no_of_agents)" />
  </node>
  <node pkg="cbf_constraints" name="master_cons_node" type="cosntraint_updater.py" output="screen">
    <param name="no_of_agents" value="$(arg no_of_agents)" />
  </node>
</launch>
